{
  "name": "valorant-discord-bot",
  "version": "1.0.0",
  "type": "module",
  "pkg": {
    "scripts": "dist/**/*.js",
    "assets": [
      "dist/**/*"
    ]
  },
  "scripts": {
    "build": "npx tsc",
    "build-exe": "nexe dist/index.js -o build/app.exe --build",
    "start": "node dist/index.js",
    "lint": "eslint . --fix",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "express": "^5.1.0",
    "open": "^10.1.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-security": "^3.0.1",
    "nexe": "^5.0.0-beta.4",
    "prettier": "^3.3.3",
    "typescript": "^5.6.2"
  }
}
